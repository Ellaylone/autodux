{
  "name": "autodux",
  "version": "2.0.0",
  "description": "Automate the Redux boilerplate.",
  "browser": "dist/index.browser.js",
  "main": "dist/index.js",
  "module": "dist/index.commonjs.js",
  "scripts": {
    "lint": "eslint source && echo 'lint finished.'",
    "test": "node source/test.js",
    "watch": "watch 'clear && npm run -s test && npm run -s lint' source",
    "prepare": "prepublish --entry-node source/index.js --entry-web source/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericelliott/autodux.git"
  },
  "keywords": [
    "Redux",
    "fp",
    "functional"
  ],
  "author": "Eric Elliott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericelliott/autodux/issues"
  },
  "homepage": "https://github.com/ericelliott/autodux#readme",
  "devDependencies": {
    "eslint": "4.2.0",
    "prepublish": "^1.1.3",
    "tape": "4.7.0",
    "watch": "1.0.2"
  },
  "dependencies": {
    "lodash.curry": "4.1.1"
  }
}
